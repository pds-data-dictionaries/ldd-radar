            "unitOfMeasureId": "null" ,
            "unitId": "null" ,
            "defaultUnitId": "null"
          , "PermissibleValueList": [
              {"PermissibleValue": {
                  "value": "Barker BPSK" ,
                  "valueMeaning": "A Barker Binary Phase Shift code is applied, usually to a pulse, to improve range resolution." ,
                  "isDeprecated": "false"
                }
             }
            , {"PermissibleValue": {
                  "value": "Linear Chirp" ,
                  "valueMeaning": "A repeating sawtooth linear frequency ramp is applied to the transmitted signal in order to resolve the target in range." ,
                  "isDeprecated": "false"
                }
             }
            , {"PermissibleValue": {
                  "value": "Non-repeating BPSK" ,
                  "valueMeaning": "A non-repeating pseudo-random code is applied to the transmitted signal using Binary Phase-Shift Keying in order to resolve the target in range." ,
                  "isDeprecated": "false"
                }
             }
            , {"PermissibleValue": {
                  "value": "Other" ,
                  "valueMeaning": "A modulation beyond a pulse and other than described above is applied to the signal." ,
                  "isDeprecated": "false"
                }
             }
            , {"PermissibleValue": {
                  "value": "Other BPSK" ,
                  "valueMeaning": "A code other than those described above is applied to the transmitted signal using Binary Phase-Shift Keying." ,
                  "isDeprecated": "false"
                }
             }
            , {"PermissibleValue": {
                  "value": "Other Chirp" ,
                  "valueMeaning": "A frequency ramp other than described above is applied to the transmitted signal." ,
                  "isDeprecated": "false"
                }
             }
            , {"PermissibleValue": {
                  "value": "Repeating LFSR BPSK" ,
                  "valueMeaning": "A maximal-length Linear Feedback Shift Register code is applied to the transmitted signal using Binary Phase-Shift Keying, in order to resolve the target in range." ,
                  "isDeprecated": "false"
                }
             }
            , {"PermissibleValue": {
                  "value": "Uncoded" ,
                  "valueMeaning": "No additional modulation beyond a pulse (if present) is applied to the signal in order to encode range information." ,
                  "isDeprecated": "false"
                }
             }
